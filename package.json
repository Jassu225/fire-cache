{
  "name": "fire-cache",
  "version": "0.1.0",
  "description": "Request-scoped, type-safe Firestore cache for Express, NestJS, and Koa.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./middleware/express": {
      "import": "./dist/middleware/express.js",
      "require": "./dist/middleware/express.js",
      "types": "./dist/middleware/express.d.ts"
    },
    "./middleware/nestjs": {
      "import": "./dist/middleware/nestjs.js",
      "require": "./dist/middleware/nestjs.js",
      "types": "./dist/middleware/nestjs.d.ts"
    },
    "./middleware/koa": {
      "import": "./dist/middleware/koa.js",
      "require": "./dist/middleware/koa.js",
      "types": "./dist/middleware/koa.d.ts"
    },
    "./core/cache": {
      "import": "./dist/core/cache.js",
      "require": "./dist/core/cache.js",
      "types": "./dist/core/cache.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build"
  },
  "keywords": [
    "firestore",
    "cache",
    "express",
    "nestjs",
    "koa",
    "typescript",
    "middleware"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-typescript": "^7.27.1",
    "@google-cloud/firestore": "^7.11.3",
    "@nestjs/common": "^11.1.5",
    "@nestjs/platform-express": "^11.1.5",
    "@nestjs/testing": "^11.1.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/koa": "^2.15.0",
    "@types/node": "^20.11.24",
    "@types/on-finished": "^2.3.5",
    "@types/supertest": "^6.0.3",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "firebase-admin": "^13.4.0",
    "jest": "^29.7.0",
    "koa": "^3.0.0",
    "rxjs": "^7.8.2",
    "supertest": "7.1.3",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3",
    "uuid": "^11.1.0"
  },
  "dependencies": {
    "on-finished": "^2.4.1"
  }
}
